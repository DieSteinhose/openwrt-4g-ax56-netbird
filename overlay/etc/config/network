config interface 'loopback'
        option device 'lo'
        option proto 'static'
        list ipaddr '127.0.0.1/8'

config globals 'globals'
        option dhcp_default_duid '000494f53a31ad754ac6b13f0d8ade8a96da'
        option ula_prefix 'fd0d:1527:a92::/48'
        option packet_steering '1'

config device
        option name 'br-lan'
        option type 'bridge'
        list ports 'lan1'
        list ports 'lan2'
        list ports 'lan3'
        list ports 'lan4'

config interface 'lan'
        option device 'br-lan'
        option proto 'static'
        list ipaddr '192.168.42.1/24'
        option ip6assign '60'

config interface 'wan'
        option device 'wan'
        option proto 'dhcp'
        option metric '10'

config interface 'wan6'
        option device 'wan'
        option proto 'dhcpv6'

config interface 'lte'
        option proto 'atc'
        option device '/dev/ttyUSB0'
        option auth '0'
        option pdp 'IP'
        option atc_debug '0'
        option metric '20'

config interface 'netbird'
        option proto 'none'
        option device 'wt0'
